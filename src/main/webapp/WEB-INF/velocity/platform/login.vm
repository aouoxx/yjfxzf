<script type="text/javascript">
	function downloadApp(type){
		url = "$!{rc.contextPath}/app/";
		if("android"==type){
			url = url + "tzfxzf.apk";
		}else{
			url = url + "tzfxzf.ipa";
		}
		window.location.href = url;
	}
</script>

