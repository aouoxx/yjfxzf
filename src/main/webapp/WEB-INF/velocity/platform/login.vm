							<td><label class="ui-label" for="password">密&nbsp;&nbsp;码:</label></td>
							<td><input id="password" name="password" class="ui-input"  type="password" /></td>
						</tr>
						<tr>
							<td>&nbsp;</td>
							<td>
							<input type="submit" class="btn-primary submit-btn" value="登录" style="margin-right:10px;">
							<input type="reset" class="btn-primary reset-btn" value="重置">
						
					  		</td>
						</tr>
					</table>
			  	</form>
			</div>
		</div>
