<body class="ui-login-body" onload="document.getElementById('username').focus()">
	<div class="ui-login-bg">
		##<div class="ui-login-header"></div>
		<div class="ui-login-content">
			<div class="content-style">
			  	<form class="ui-form ui-login-form" id="LoginForm" action="$!{rc.contextPath}/platform/login" method="post">
			  		<input type="hidden" id="callback" name="callback" value="$!{rc.contextPath}/" />
			  		<table border="0" cellpadding="0" cellspacing="0">
						<tr><td>&nbsp;</td><td><span class="login-welcome">欢迎登录</span></td></tr>
						<tr>
							<td><label class="ui-label" for="username">用&nbsp;&nbsp;户:</label></td>
